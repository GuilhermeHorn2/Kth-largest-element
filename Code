#include <iostream>
#include <cmath>
//

//
using namespace std;

int talos(int l[],int n,int k){
    int p = 0;
    int r;
    while(p < k){
        int m = l[0];
        int s;
        for(int i = 0;i < n;i++){
            if(l[i] > m){
                m = l[i];
                s = i;
            }
        }
        l[s] = 0;
        r = m;
        p++;
    }
    return r;
}



int main(){
    /*\n*/
    









return 0;

}
